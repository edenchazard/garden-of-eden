{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "fmt": "prettier --write .",
    "fmt:check": "prettier . --check",
    "test:unit": "vitest",
    "ts:check": "nuxi typecheck",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up",
    "dev:docker:cleanup": "docker-compose down --rmi all --remove-orphans",
    "dev:docker:cleanup:all": "docker-compose down --rmi all --volumes --remove-orphans",
    "dev:docker:migrate": "docker compose exec hatchery bash -c 'npm run db:migrate'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@nuxtjs/color-mode": "^3.4.2",
    "@nuxtjs/robots": "^4.0.2",
    "@sidebase/nuxt-auth": "^0.8.1",
    "@vueuse/core": "^10.11.0",
    "bullmq": "^5.21.2",
    "chart.js": "^4.4.3",
    "chartjs-adapter-luxon": "^1.3.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "floating-vue": "^5.2.2",
    "luxon": "^3.5.0",
    "mariadb": "^3.3.1",
    "mysql2": "^3.11.0",
    "next-auth": "^4.21.1",
    "nuxt": "^3.12.4",
    "nuxt-security": "^2.0.0-rc.9",
    "vue": "latest",
    "vue-chartjs": "^5.3.1",
    "vue3-toastify": "^0.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.0",
    "@nuxt/test-utils": "^3.14.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.24.2",
    "happy-dom": "^14.12.3",
    "nuxt-cron": "^1.6.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.16.2",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.4"
  }
}
