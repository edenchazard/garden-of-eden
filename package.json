{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "fmt": "prettier --write .",
    "fmt:check": "prettier . --check",
    "test:unit": "vitest",
    "ts:check": "nuxi typecheck",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up",
    "dev:docker:cleanup": "docker-compose down --rmi all --remove-orphans",
    "dev:docker:cleanup:all": "docker-compose down --rmi all --volumes --remove-orphans",
    "dev:docker:migrate": "docker compose exec hatchery bash -c 'npm run db:migrate'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@nuxt/image": "^1.9.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/robots": "^5.2.8",
    "@sidebase/nuxt-auth": "^0.10.1",
    "@tailwindcss/vite": "^4.0.9",
    "@vueuse/core": "^10.11.0",
    "bullmq": "^5.43.1",
    "chart.js": "^4.4.8",
    "chartjs-adapter-luxon": "^1.3.1",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.0",
    "floating-vue": "^5.2.2",
    "luxon": "^3.5.0",
    "mariadb": "^3.4.0",
    "mysql2": "^3.13.0",
    "next-auth": "~4.21.1",
    "node-webpmux": "^3.2.0",
    "nuxt": "^3.16.0",
    "nuxt-security": "^2.2.0",
    "ofetch": "^1.4.1",
    "sharp": "^0.33.5",
    "vue": "latest",
    "vue-chartjs": "^5.3.2",
    "vue3-toastify": "^0.2.8",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/test-utils": "^3.17.2",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.30.5",
    "esbuild": "^0.24.0",
    "happy-dom": "^17.4.4",
    "nuxt-cron": "^1.8.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.9",
    "tsx": "^4.19.3",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  },
  "overrides": {
    "sharp": "$sharp"
  }
}
